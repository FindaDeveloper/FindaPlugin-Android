<idea-plugin>
    <id>com.github.kimdohun0104.findatemplatepluginandroid</id>
    <name>FindaTemplatePlugin-Android</name>
    <vendor>kimdohun0104</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <actions>
        <action id="org.jetbrains.plugins.template.actions.CreateFindaTemplateAction"
                class="org.jetbrains.plugins.template.actions.CreateFindaTemplateAction"
                text="New Finda Template"
                description="This is Finda"
                icon="Icons.FindaLogo">
            <add-to-group group-id="NewGroup" anchor="first" />
        </action>
    </actions>
</idea-plugin>
