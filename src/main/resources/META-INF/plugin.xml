<idea-plugin>
    <id>com.github.kimdohun0104.findatemplatepluginandroid</id>
    <name>FindaTemplatePlugin-Android</name>
    <vendor>kimdohun0104</vendor>

    <depends>com.intellij.modules.platform</depends>

    <actions>
        <action id="com.github.kimdohun0104.findatemplatepluginandroid.actions.CreateFindaTemplateAction"
                class="com.github.kimdohun0104.findatemplatepluginandroid.actions.CreateFindaTemplateAction"
                text="New Finda Template"
                description="This is Finda"
                icon="Icons.FindaLogo">
            <add-to-group group-id="NewGroup" anchor="first"/>
        </action>
    </actions>
</idea-plugin>
